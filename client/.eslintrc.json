{
    "env": {
        "browser": true,
        "es6": true,
        "node": true
    },
    "extends": ["eslint:recommended", "plugin:react/recommended"],
    "globals": {
        "Atomics": "readonly",
        "SharedArrayBuffer": "readonly"
    },
    "parser": "babel-eslint",
    "parserOptions": {
        "ecmaFeatures": {
            "jsx": true
        },
        "ecmaVersion": 2018,
        "sourceType": "module"
    },
    "plugins": ["react"],
    "rules": {
        "no-undef": "warn",
        "no-unused-vars": "warn",
        "no-case-declarations": "off",
        "react/prop-types": "off",
        "react/no-unescaped-entities": "off",
        "no-console": "off",
        "no-empty": "off",
        "react/jsx-key": "off"
    }
}
